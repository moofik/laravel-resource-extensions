  "keywords": [
    "moofik",
    "laravel",
    "resource",
    "resources",
    "api",
    "api-resources"
  ],
  "homepage": "https://github.com/moofik/laravel-resource-extensions",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Orlovsky",
      "email": "moofik12@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/container": ">=7.0",
    "illuminate/contracts": ">=7.0",
    "illuminate/database": ">=7.0",
    "illuminate/pagination": ">=7.0",
    "illuminate/http": ">=7.0",
    "illuminate/support": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "Moofik\\LaravelResourceExtenstion\\": "src",
      "Moofik\\LaravelResourceExtenstion\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.5"
  },
  "scripts": {
    "test": "phpunit"
  }
}
